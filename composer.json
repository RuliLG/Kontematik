{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "algolia/algoliasearch-client-php": "^3.0",
        "blade-ui-kit/blade-heroicons": "^1.3.1",
        "blade-ui-kit/blade-icons": "^1.3.0",
        "codeat3/blade-eos-icons": "^1.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hubspot/api-client": "^3.0",
        "joggapp/laravel-google-translate": "^8.0",
        "laravel/framework": "^9.0",
        "laravel/scout": "^9.1",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/spark-stripe": "^1.0",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.4",
        "mailjet/laravel-mailjet": "^3.0",
        "php-science/textrank": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.1",
        "laravel/dusk": "^6.15",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/phpinsights": "^2.3",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://spark.laravel.com"
        }
    ]
}
